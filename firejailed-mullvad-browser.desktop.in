[Desktop Entry]
Version=1.1
Type=Application
Name=Mullvad Browser
GenericName=Web Browser
Comment=Mullvad Browser is +1 for privacy and -1 for mass surveillance
Icon=HOME/.firejailed-mullvad-browser/Browser/browser/chrome/icons/default/default128.png
Exec=sh -c 'firejail --profile=firejailed-mullvad-browser "HOME/Browser/start-mullvad-browser" --name="Mullvad Browser"'
Categories=Network;Security;WebBrowser;
Keywords=MBB;
StartupNotify=true
StartupWMClass=Mullvad Browser
